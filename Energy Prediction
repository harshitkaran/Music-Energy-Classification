      "source": [
        "def _pca(extracted_features):\n",
        "  df = pd.DataFrame(extracted_features)\n",
        "  \n",
        "  pca = PCA(n_components=0.99)\n",
        "  principal_features = pca.fit_transform(df)\n",
        "  df = pd.DataFrame(principal_features)\n",
        "  return df"
      ]
